{
  "name": "quest4kids",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run build && npm run typeorm migration:generate -- -d typeorm.config.ts",
    "migration:run": "npm run build && npm run typeorm migration:run -- -d typeorm.config.ts",
    "migration:revert": "npm run build && npm run typeorm migration:revert -- -d typeorm.config.ts",
    "e2e:db:up": "docker compose -f test/docker-compose.e2e.yml up -d",
    "e2e:db:ps": "docker compose -f test/docker-compose.e2e.yml ps",
    "e2e:db:logs": "docker logs -f quest4kids_pg_test",
    "e2e:db:down": "docker compose -f test/docker-compose.e2e.yml down -v",
    "db:seed:test": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.e2e node -r ts-node/register scripts/seed.ts",
    "test:e2e": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.e2e node -r dotenv/config ./node_modules/.bin/jest --config ./test/jest-e2e.config.ts --runInBand",
    "test:e2e:seeded": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.e2e E2E_CLEAN=1 E2E_SEED=e2e E2E_USERS=3 E2E_LABELS=5 E2E_TASKS=5 E2E_BADGES=2 E2E_USER_BADGES=1 E2E_NOTIFICATIONS=1 E2E_MESSAGES=1 node -r dotenv/config ./node_modules/.bin/jest --config ./test/jest-e2e.config.ts --runInBand",
    "test:e2e:reset": "npm run e2e:db:down && npm run e2e:db:up && npm run test:e2e:seeded",
    "test:e2e:tasks": "npm run test:e2e -- -t \"Tasks CRUD\"",
    "test:e2e:seeded:tasks": "npm run test:e2e:seeded -- -t \"Tasks CRUD\""
  },
  "dependencies": {
    "@bull-board/api": "^6.9.6",
    "@bull-board/express": "^6.9.6",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^11.1.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.0.17",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.0.5",
    "@nestjs/websockets": "^11.0.17",
    "@types/cron": "^2.0.1",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.52.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parse": "^5.6.0",
    "express": "^5.1.0",
    "i": "^0.3.7",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "openai": "^5.6.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "telegraf": "^4.16.3",
    "typeorm": "^0.3.20",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@nestjs/typeorm": "^11.0.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.1.2",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
